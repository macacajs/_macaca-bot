{
  "name": "macaca-bot",
  "version": "1.0.0",
  "description": "github Github bot to resolve the issues and PR of your repository",
  "private": true,
  "dependencies": {
    "@octokit/rest": "^15.4.1",
    "cryptiles": "^4.1.1",
    "egg": "^2.2.1",
    "egg-scripts": "^2.5.0",
    "google-translate-api": "^2.3.0",
    "xutil": "^1.0.11"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "git-contributor": "^1.0.8",
    "husky": "^1.3.1",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --port=9500 --daemon --title=egg-server-macaca-bot",
    "stop": "egg-scripts stop --title=egg-server-macaca-bot",
    "dev": "egg-bin dev --port=9500",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "contributor": "git-contributor"
  },
  "ci": {
    "version": "8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:macacajs/macaca-bot.git"
  },
  "license": "MIT"
}
